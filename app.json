{
  "expo": {
    "name": "GamePlan",
    "slug": "gameplan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "gameplan",
    "newArchEnabled": true,
    "experiments": {
      "baseUrl": "/gameplan"
    },
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gameplan.app",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gameplan.app",
      "googleServicesFile": "./google-services.json",
      "permissions": ["CALENDAR", "READ_CALENDAR", "WRITE_CALENDAR"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-calendar"
    ]
  }
}
